{% extends _request.isAjax ? "blank.twig" : "base.twig" %}
{%block body%}

<div role="pdf-editor">
{%for page in fileView%}
	<div role="pdf-editor-page" data-image="/managment/form/add/finish/image/{{page}}" />
{%endfor%}

{%for field in entity.fields%}
	<div role="pdf-editor-component" data-name="{{field.name}}" data-position-x="{{field.positionX}}" data-position-y="{{field.positionY}}" data-page="{{field.page}}" />
{%endfor%}

</div>

{%endblock%}